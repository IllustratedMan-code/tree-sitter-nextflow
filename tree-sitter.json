{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "nextflow",
      "camelcase": "Nextflow",
      "title": "Nextflow",
      "scope": "source.nextflow",
      "path": ".",
      "file-types": [
        "nextflow",
        "nf"
      ],
      "injection-regex": "^nextflow$",
      "class-name": "TreeSitterNextflow"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A tree-sitter grammar for the nextflow language",
    "authors": [
      {
        "name": "IllustratedMan-code"
      }
    ],
    "links": {
      "repository": "https://github.com/IllustratedMan-code/tree-sitter-nextflow"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
